        function denmanhinh()
            local snum2 = 1 
                repeat
                    mSleep(1000);
                    local x, y = findColorInRegionFuzzy(0x000000, 98, 210, 200, 220, 210);  
                    if x1 ~= -1 and y1 ~= -1 then
                    mSleep(20);
                        local x, y = findColorInRegionFuzzy(0x000000, 98, 210, 670, 220, 680);  
                        if x ~= -1 and y ~= -1 then
                            mSleep(3000);
                            if snum2 == 11 then
                                closeApp("com.wemadeplay.anipangmatch");
                                mSleep(1000);
                                pressHomeKey()
                                mSleep(1000);
                                break
                            end
                            -- toast("NamZed.Com: Đen Màn Hình Vãi Lồn Lần Thứ: "..snum2.."/10")
                        end
                    else
                        mSleep(500);
                        break
                    end
                snum2 = snum2 + 1
                until snum2 == 20
                mSleep(100);
        end

