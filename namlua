        function pve()
            while true do
                local x, y = findColorInRegionFuzzy(0xbc6bd1, 98,442,833,478,849)
                if x ~= -1 and y ~= -1 then
                    checkgauanba()----------------
                    mSleep(timeslep);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    chomain1()----------
                    mSleep(20);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    checkgaudenanba()----------------
                    mSleep(timeslep);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    thomain1()----------
                    mSleep(20);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    checkkhianba()-------------
                    mSleep(timeslep);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    gamain1()----------
                    mSleep(20);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    checkgaanba()--------------
                    mSleep(timeslep);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    khimain1()----------
                    mSleep(20);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    checkthoanba()----------------
                    mSleep(timeslep);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    gaudenmain1()----------
                    mSleep(20);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    checkchoanba()------------------
                    mSleep(timeslep);
                    -- toast("NamZed.Com")
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                    gaumain1()----------
                    mSleep(20);
                    ngoisao()
                    mSleep(20);
                    phatno()
                    mSleep(20);
                    mattrang()
                    mSleep(20);
                else
                    -- toast("NamZed.Com: Loading......")
                    ngoaigame()
                end
                mSleep(50)
            end
        end

        function pvp()
            checkgauanba()----------------
            mSleep(timeslep);
            checkgaudenanba()----------------
            mSleep(timeslep);
            checkkhianba()-------------
            mSleep(timeslep);
            checkgaanba()--------------
            mSleep(timeslep);
            checkthoanba()----------------
            mSleep(timeslep);
            checkchoanba()------------------
            mSleep(timeslep);
            -- toast("NamZed.Com")
        end



        -- Bắt đầu chạy luồng hiện toast
        if choice == 0 then
            while (true) do
                pvp()
                mSleep(50)
                runShowToast()
            end
        elseif choice == 1 then
            while (true) do
                pve()
                mSleep(50)
                runShowToast()
            end
        elseif choice == 2 then
            lua_exit()
        end


    end
