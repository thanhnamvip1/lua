    local ts              = require('ts')
    local json            = ts.json
    local function showToast()
        while true do
            local t = 15
            mSleep(500)
            for i = 1, 10 do
                for i = 1, t do
                    local j = t - i
                    -- toast('NamZed.Com:'..('   '):rep(i)..'➳'..('   '):rep(j)..'⊙')
                    toast('NamZed.Com')
                    mSleep(3000)
                end
            end
        end
    end
    local thread = require('thread')
    local runShowToast = thread.create(function()
        showToast()
    end)


    choice = dialogRet("Chọn Tool", "PVP", "PVE", "Thoát Tool", 0);
    if choice == 0 then   
        timeslep = 2
        nam1 = 366
        nam2 = 508
        nam3 = 844
    elseif choice == 1 then  
        -- dialog("Bản PVE hiện có hack PVE k mất tym auto qua màn",{timeout = 10,title = "Thông Báo",button = "ok"})
        timeslep = 20
        nam1 = 230
        nam2 = 539
        nam3 = 760
    elseif choice == 2 then  
        lua_exit()
    end

    while true
        do
        function gau1()
            local x, y = findMultiColorInRegionFuzzy(0xa1dc00,"60|120|0xa1dc00,60|0|0xa1dc00,0|60|0xa1dc00",95,2,nam1,nam2,nam3)
            if x ~= -1 and y ~= -1 then
                local x1 = x + 60
                local y1 = y + 120
                local x2 = x + 60
                local y2 = y + 60
                moveTo(x1,y1,x2,y2,20,200)
            end
        end
