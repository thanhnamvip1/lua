        function mogamelen1() -- mở acc
            local flag = appIsRunning("com.wemadeplay.anipangmatch"); 
            if flag  == 0 then     
                -- toast("NamZed: Vao Game")
                runApp("com.wemadeplay.anipangmatch")           
            end
            switchApp("com.wemadeplay.anipangmatch")
        end

